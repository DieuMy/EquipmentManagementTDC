<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    style="@style/CardViewListEquipmentScreenLayout_Linearlayout"
    android:layout_margin="@dimen/CardViewListEquipmentScreenLayoutMargin"
    app:cardCornerRadius="@dimen/CardViewListEquipmentScreenCardCornerRadius"
    app:cardElevation="@dimen/CardViewListEquipmentScreenCardElevation">

    <LinearLayout style="@style/ListEquipmentScreenCardViewLinearLayout">

        <de.hdodenhof.circleimageview.CircleImageView
            android:layout_width="70dp"
            android:layout_height="70dp"
            android:layout_gravity="center"
            android:src="@drawable/laptop_icon"
            android:elevation="@dimen/homeScreenElevationOfImage"
            android:layout_marginLeft="15dp"/>

        <TextView
            android:id="@+id/listequipmentscreentextviewNameEquipment"
            style="@style/ListEquipmentScreenCardViewTextView" />
    </LinearLayout>


</androidx.cardview.widget.CardView>