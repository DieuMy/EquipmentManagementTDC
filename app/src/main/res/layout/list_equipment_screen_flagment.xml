<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    tools:context=".MainActivity"
    style="@style/ListEquipmentScreenCardViewLinearLayout">
    <com.google.android.material.appbar.AppBarLayout
        style="@style/AppBarActionStyle">
        <TextView
            style="@style/AppBarLayoutTextViewScreenName"
            android:text="@string/ListEquipmentScreenNameScreen"/>
    </com.google.android.material.appbar.AppBarLayout>

    <androidx.recyclerview.widget.RecyclerView
        android:background="@drawable/list_equipment_screen_background"
        android:id="@+id/ListEquipmentScreenRecycleViewFunction"
        style="@style/ListEquipmentScreenRecycleView"></androidx.recyclerview.widget.RecyclerView>

</LinearLayout>